<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:L7p="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:All wsp:Usage="Required">
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="========================================"/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="Extract metrics data from buffer and push to data lake."/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="========================================"/>
        </L7p:CommentAssertion>
        <L7p:AuditAssertion>
            <L7p:Enabled booleanValue="false"/>
        </L7p:AuditAssertion>
        <wsp:OneOrMore wsp:Usage="Required">
            <wsp:All wsp:Usage="Required">
                <L7p:ComparisonAssertion>
                    <L7p:CaseSensitive booleanValue="false"/>
                    <L7p:Expression1 stringValue="${gateway.portal.analytics.datalake.hostname}"/>
                    <L7p:Operator operatorNull="null"/>
                    <L7p:Predicates predicates="included">
                        <L7p:item dataType="included">
                            <L7p:Type variableDataType="string"/>
                        </L7p:item>
                        <L7p:item binary="included">
                            <L7p:RightValue stringValue="not installed"/>
                        </L7p:item>
                    </L7p:Predicates>
                </L7p:ComparisonAssertion>
                <L7p:CommentAssertion>
                    <L7p:Comment stringValue="Datalake not configured. Do nothing."/>
                </L7p:CommentAssertion>
            </wsp:All>
            <wsp:All wsp:Usage="Required">
                <L7p:SetVariable>
                    <L7p:Base64Expression stringValue=""/>
                    <L7p:VariableToSet stringValue="empty"/>
                </L7p:SetVariable>
                <L7p:BufferData>
                    <L7p:BufferName stringValue="gooddata-metrics"/>
                    <L7p:MaxAgeMillis longValue="0"/>
                    <L7p:MaxSizeBytes longValue="0"/>
                    <L7p:NewDataVarName stringValue="empty"/>
                </L7p:BufferData>
                <L7p:AuditDetailAssertion>
                    <L7p:Detail stringValue="buffer.wasFull = ${buffer.wasFull}buffer.wasExtrated = ${buffer.wasExtracted}buffer.newSize.bytes = ${buffer.newSize.bytes}buffer.newAge.millis = ${buffer.newAge.millis}buffer.extractedMessage.mainpart.size = ${buffer.extractedMessage.mainpart.size}"/>
                    <L7p:Level stringValue="FINEST"/>
                </L7p:AuditDetailAssertion>
                <wsp:OneOrMore wsp:Usage="Required">
                    <wsp:All wsp:Usage="Required">
                        <L7p:ComparisonAssertion>
                            <L7p:CaseSensitive booleanValue="false"/>
                            <L7p:Expression1 stringValue="${buffer.wasExtracted}"/>
                            <L7p:Operator operatorNull="null"/>
                            <L7p:Predicates predicates="included">
                                <L7p:item dataType="included">
                                    <L7p:Type variableDataType="boolean"/>
                                </L7p:item>
                                <L7p:item binary="included">
                                    <L7p:CaseSensitive booleanValue="false"/>
                                    <L7p:RightValue stringValue="true"/>
                                </L7p:item>
                            </L7p:Predicates>
                        </L7p:ComparisonAssertion>
                        <L7p:AuditDetailAssertion>
                            <L7p:Detail stringValue="Metrics data successfully extracted from buffer."/>
                            <L7p:Level stringValue="FINEST"/>
                        </L7p:AuditDetailAssertion>
                        <wsp:OneOrMore wsp:Usage="Required">
                            <wsp:All wsp:Usage="Required">
                                <L7p:ComparisonAssertion>
                                    <L7p:CaseSensitive booleanValue="false"/>
                                    <L7p:Expression1 stringValue="${buffer.extractedMessage.mainpart.size}"/>
                                    <L7p:Operator operatorNull="null"/>
                                    <L7p:Predicates predicates="included">
                                    <L7p:item dataType="included">
                                    <L7p:Type variableDataType="int"/>
                                    </L7p:item>
                                    <L7p:item binary="included">
                                    <L7p:RightValue stringValue="0"/>
                                    </L7p:item>
                                    </L7p:Predicates>
                                </L7p:ComparisonAssertion>
                                <L7p:CommentAssertion>
                                    <L7p:Comment stringValue="Buffer is empty. Do nothing."/>
                                </L7p:CommentAssertion>
                            </wsp:All>
                            <wsp:All wsp:Usage="Required">
                                <L7p:ContentType>
                                    <L7p:ChangeContentType booleanValue="true"/>
                                    <L7p:NewContentTypeValue stringValue="text/plain; charset=utf-8"/>
                                    <L7p:OtherTargetMessageVariable stringValue="buffer.extractedMessage"/>
                                    <L7p:ReinitializeMessage booleanValue="true"/>
                                    <L7p:Target target="OTHER"/>
                                </L7p:ContentType>
                                <L7p:EncodeDecode>
                                    <L7p:SourceVariableName stringValue="buffer.extractedMessage"/>
                                    <L7p:TargetContentType stringValue="application/gzip"/>
                                    <L7p:TargetDataType variableDataType="message"/>
                                    <L7p:TargetVariableName stringValue="gzipped"/>
                                    <L7p:TransformType transformType="GZIP"/>
                                </L7p:EncodeDecode>
                                <L7p:Encapsulated>
                                    <L7p:EncapsulatedAssertionConfigGuid stringValue="ed1e3112-5df8-4769-8436-09ddb1362f88"/>
                                    <L7p:EncapsulatedAssertionConfigName stringValue="portalosencass"/>
                                    <L7p:Parameters mapValue="included">
                                    <L7p:entry>
                                    <L7p:key stringValue="defaultHost"/>
                                    <L7p:value stringValue="${gateway.portal.config.datalake.hostname}"/>
                                    </L7p:entry>
                                    <L7p:entry>
                                    <L7p:key stringValue="defaultPort"/>
                                    <L7p:value stringValue="9447"/>
                                    </L7p:entry>
                                    <L7p:entry>
                                    <L7p:key stringValue="hostName"/>
                                    <L7p:value stringValue="${gateway.portal.config.dssg.datalake.host}"/>
                                    </L7p:entry>
                                    <L7p:entry>
                                    <L7p:key stringValue="port"/>
                                    <L7p:value stringValue="${gateway.portal.config.dssg.datalake.port}"/>
                                    </L7p:entry>
                                    </L7p:Parameters>
                                </L7p:Encapsulated>
                                <L7p:HttpRoutingAssertion>
                                    <L7p:FailOnErrorStatus booleanValue="false"/>
                                    <L7p:HttpMethod httpMethod="PUT"/>
                                    <L7p:KeyAlias stringValue="portalman"/>
                                    <L7p:NonDefaultKeystoreId goidValue="00000000000000000000000000000002"/>
                                    <L7p:ProtectedServiceUrl stringValue="https://${outputHost}:${outputPort}/datalake/${gateway.portal.config.identifier}/${ssgnode.id}"/>
                                    <L7p:ProxyPassword stringValueNull="null"/>
                                    <L7p:ProxyUsername stringValueNull="null"/>
                                    <L7p:RequestHeaderRules httpPassthroughRuleSet="included">
                                    <L7p:ForwardAll booleanValue="true"/>
                                    <L7p:Rules httpPassthroughRules="included">
                                    <L7p:item httpPassthroughRule="included">
                                    <L7p:Name stringValue="Cookie"/>
                                    </L7p:item>
                                    <L7p:item httpPassthroughRule="included">
                                    <L7p:Name stringValue="SOAPAction"/>
                                    </L7p:item>
                                    </L7p:Rules>
                                    </L7p:RequestHeaderRules>
                                    <L7p:RequestMsgSrc stringValue="gzipped"/>
                                    <L7p:RequestParamRules httpPassthroughRuleSet="included">
                                    <L7p:ForwardAll booleanValue="true"/>
                                    <L7p:Rules httpPassthroughRules="included"/>
                                    </L7p:RequestParamRules>
                                    <L7p:ResponseHeaderRules httpPassthroughRuleSet="included">
                                    <L7p:ForwardAll booleanValue="true"/>
                                    <L7p:Rules httpPassthroughRules="included">
                                    <L7p:item httpPassthroughRule="included">
                                    <L7p:Name stringValue="Set-Cookie"/>
                                    </L7p:item>
                                    </L7p:Rules>
                                    </L7p:ResponseHeaderRules>
                                    <L7p:ResponseMsgDest stringValue="datalakeResponse"/>
                                    <L7p:SamlAssertionVersion intValue="2"/>
                                    <L7p:UsesDefaultKeyStore booleanValue="false"/>
                                </L7p:HttpRoutingAssertion>
                                <L7p:CommentAssertion>
                                    <L7p:Comment stringValue="========================================"/>
                                </L7p:CommentAssertion>
                                <L7p:CommentAssertion>
                                    <L7p:Comment stringValue="Check HTTP response code."/>
                                </L7p:CommentAssertion>
                                <L7p:CommentAssertion>
                                    <L7p:Comment stringValue="========================================"/>
                                </L7p:CommentAssertion>
                                <wsp:OneOrMore wsp:Usage="Required">
                                    <wsp:All wsp:Usage="Required">
                                    <L7p:ComparisonAssertion>
                                    <L7p:CaseSensitive booleanValue="false"/>
                                    <L7p:Expression1 stringValue="${datalakeResponse.http.status}"/>
                                    <L7p:Operator operatorNull="null"/>
                                    <L7p:Predicates predicates="included">
                                    <L7p:item dataType="included">
                                    <L7p:Type variableDataType="int"/>
                                    </L7p:item>
                                    <L7p:item binary="included">
                                    <L7p:RightValue stringValue="200"/>
                                    </L7p:item>
                                    </L7p:Predicates>
                                    </L7p:ComparisonAssertion>
                                    <L7p:AuditDetailAssertion>
                                    <L7p:Detail stringValue="Metrics data successfully pushed to data lake:Response body = ${datalakeResponse.mainpart}"/>
                                    <L7p:Level stringValue="FINEST"/>
                                    </L7p:AuditDetailAssertion>
                                    </wsp:All>
                                    <wsp:All wsp:Usage="Required">
                                    <L7p:AuditDetailAssertion>
                                    <L7p:Detail stringValue="Failed to push metrics data to data lake:Response HTTP status = ${datalakeResponse.http.status}Response body = ${datalakeResponse.mainpart}"/>
                                    <L7p:Level stringValue="WARNING"/>
                                    </L7p:AuditDetailAssertion>
                                    </wsp:All>
                                </wsp:OneOrMore>
                            </wsp:All>
                        </wsp:OneOrMore>
                    </wsp:All>
                    <wsp:All wsp:Usage="Required">
                        <L7p:ComparisonAssertion>
                            <L7p:CaseSensitive booleanValue="false"/>
                            <L7p:Expression1 stringValue="${buffer.wasExtracted}"/>
                            <L7p:Operator operatorNull="null"/>
                            <L7p:Predicates predicates="included">
                                <L7p:item dataType="included">
                                    <L7p:Type variableDataType="boolean"/>
                                </L7p:item>
                                <L7p:item binary="included">
                                    <L7p:CaseSensitive booleanValue="false"/>
                                    <L7p:RightValue stringValue="false"/>
                                </L7p:item>
                            </L7p:Predicates>
                        </L7p:ComparisonAssertion>
                        <L7p:AuditDetailAssertion>
                            <L7p:Detail stringValue="Failed to extract metrics data from buffer."/>
                            <L7p:Level stringValue="WARNING"/>
                        </L7p:AuditDetailAssertion>
                    </wsp:All>
                    <wsp:All wsp:Usage="Required">
                        <L7p:AuditDetailAssertion>
                            <L7p:Detail stringValue="Unexpected error."/>
                            <L7p:Level stringValue="WARNING"/>
                        </L7p:AuditDetailAssertion>
                    </wsp:All>
                </wsp:OneOrMore>
            </wsp:All>
            <wsp:All wsp:Usage="Required">
                <L7p:AuditDetailAssertion>
                    <L7p:Detail stringValue="Unexpected error."/>
                    <L7p:Level stringValue="WARNING"/>
                </L7p:AuditDetailAssertion>
                <L7p:FalseAssertion/>
            </wsp:All>
        </wsp:OneOrMore>
    </wsp:All>
</wsp:Policy>