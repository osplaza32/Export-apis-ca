                </wsp:All>
                <L7p:SetVariable>
                    <L7p:Base64Expression stringValue="JHtkZWZhdWx0SG9zdH0="/>
                    <L7p:VariableToSet stringValue="outputHost"/>
                </L7p:SetVariable>
            </wsp:OneOrMore>
