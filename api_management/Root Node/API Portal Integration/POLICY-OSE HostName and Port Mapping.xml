                    </L7p:ComparisonAssertion>
                    <L7p:SetVariable>
                        <L7p:Base64Expression stringValue="JHtwb3J0fQ=="/>
                        <L7p:VariableToSet stringValue="outputPort"/>
                    </L7p:SetVariable>
                </wsp:All>
                <L7p:SetVariable>
                    <L7p:Base64Expression stringValue="JHtkZWZhdWx0UG9ydH0="/>
