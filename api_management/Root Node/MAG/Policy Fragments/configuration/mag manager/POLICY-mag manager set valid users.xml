<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:L7p="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:All wsp:Usage="Required">
        <L7p:CommentAssertion>
            <L7p:Comment stringValue=" ============================================"/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue=" === ***  THIS POLICY IS READ ONLY ***"/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue=" === If you have the need to customize this policy"/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue=" === please contact CA APIM Support so we can improve customization capability"/>
        </L7p:CommentAssertion>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue=" ============================================"/>
        </L7p:CommentAssertion>
        <L7p:Encapsulated>
            <L7p:EncapsulatedAssertionConfigGuid stringValue="4e4bc164-3fa2-4b15-ba65-31cef45b84b0"/>
            <L7p:EncapsulatedAssertionConfigName stringValue="OTK Session GET"/>
            <L7p:Parameters mapValue="included">
                <L7p:entry>
                    <L7p:key stringValue="cacheAge"/>
                    <L7p:value stringValue=""/>
                </L7p:entry>
                <L7p:entry>
                    <L7p:key stringValue="cacheId"/>
                    <L7p:value stringValue="${cacheName}"/>
                </L7p:entry>
                <L7p:entry>
                    <L7p:key stringValue="cacheKey"/>
                    <L7p:value stringValue="${sessionID}"/>
                </L7p:entry>
            </L7p:Parameters>
        </L7p:Encapsulated>
        <L7p:ResponseXpathAssertion>
            <L7p:VariablePrefix stringValue="xpathSessionValue"/>
            <L7p:XmlMsgSrc stringValue="resp"/>
            <L7p:XpathExpression xpathExpressionValue="included">
                <L7p:Expression stringValue="/ns:found/ns:value"/>
                <L7p:Namespaces mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="ns"/>
                        <L7p:value stringValue="http://ns.l7tech.com/2012/11/otk-session"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="s"/>
                        <L7p:value stringValue="http://schemas.xmlsoap.org/soap/envelope/"/>
                    </L7p:entry>
                </L7p:Namespaces>
                <L7p:XpathVersion xpathVersion="XPATH_1_0"/>
            </L7p:XpathExpression>
        </L7p:ResponseXpathAssertion>
        <L7p:EncodeDecode>
            <L7p:SourceVariableName stringValue="xpathSessionValue.result"/>
            <L7p:TargetContentType stringValue="text/xml; charset=utf-8"/>
            <L7p:TargetDataType variableDataType="message"/>
            <L7p:TargetVariableName stringValue="userSession"/>
            <L7p:TransformType transformType="URL_DECODE"/>
        </L7p:EncodeDecode>
        <L7p:ResponseXpathAssertion>
            <L7p:VariablePrefix stringValue="xpathSessionUser"/>
            <L7p:XmlMsgSrc stringValue="userSession"/>
            <L7p:XpathExpression xpathExpressionValue="included">
                <L7p:Expression stringValue="/usersession/user"/>
                <L7p:Namespaces mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="ns"/>
                        <L7p:value stringValue="http://ns.l7tech.com/2012/11/otk-session"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="s"/>
                        <L7p:value stringValue="http://schemas.xmlsoap.org/soap/envelope/"/>
                    </L7p:entry>
                </L7p:Namespaces>
                <L7p:XpathVersion xpathVersion="XPATH_1_0"/>
            </L7p:XpathExpression>
        </L7p:ResponseXpathAssertion>
        <L7p:ResponseXpathAssertion>
            <L7p:VariablePrefix stringValue="xpathSessionRole"/>
            <L7p:XmlMsgSrc stringValue="userSession"/>
            <L7p:XpathExpression xpathExpressionValue="included">
                <L7p:Expression stringValue="/usersession/role"/>
                <L7p:Namespaces mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="ns"/>
                        <L7p:value stringValue="http://ns.l7tech.com/2012/11/otk-session"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="s"/>
                        <L7p:value stringValue="http://schemas.xmlsoap.org/soap/envelope/"/>
                    </L7p:entry>
                </L7p:Namespaces>
                <L7p:XpathVersion xpathVersion="XPATH_1_0"/>
            </L7p:XpathExpression>
        </L7p:ResponseXpathAssertion>
        <L7p:ResponseXpathAssertion>
            <L7p:VariablePrefix stringValue="xpathSessionAccessToken"/>
            <L7p:XmlMsgSrc stringValue="userSession"/>
            <L7p:XpathExpression xpathExpressionValue="included">
                <L7p:Expression stringValue="/usersession/access_token"/>
                <L7p:Namespaces mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="ns"/>
                        <L7p:value stringValue="http://ns.l7tech.com/2012/11/otk-session"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="s"/>
                        <L7p:value stringValue="http://schemas.xmlsoap.org/soap/envelope/"/>
                    </L7p:entry>
                </L7p:Namespaces>
                <L7p:XpathVersion xpathVersion="XPATH_1_0"/>
            </L7p:XpathExpression>
        </L7p:ResponseXpathAssertion>
        <L7p:ResponseXpathAssertion>
            <L7p:VariablePrefix stringValue="xpathSynchToken"/>
            <L7p:XmlMsgSrc stringValue="userSession"/>
            <L7p:XpathExpression xpathExpressionValue="included">
                <L7p:Expression stringValue="/usersession/synchToken"/>
                <L7p:Namespaces mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="ns"/>
                        <L7p:value stringValue="http://ns.l7tech.com/2012/11/otk-session"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="s"/>
                        <L7p:value stringValue="http://schemas.xmlsoap.org/soap/envelope/"/>
                    </L7p:entry>
                </L7p:Namespaces>
                <L7p:XpathVersion xpathVersion="XPATH_1_0"/>
            </L7p:XpathExpression>
        </L7p:ResponseXpathAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHt4cGF0aFNlc3Npb25Vc2VyLnJlc3VsdH0="/>
            <L7p:VariableToSet stringValue="current.username"/>
        </L7p:SetVariable>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHt4cGF0aFNlc3Npb25Sb2xlLnJlc3VsdH0="/>
            <L7p:VariableToSet stringValue="current.user.role"/>
        </L7p:SetVariable>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHt4cGF0aFNlc3Npb25BY2Nlc3NUb2tlbi5yZXN1bHR9"/>
            <L7p:VariableToSet stringValue="access_token"/>
        </L7p:SetVariable>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHtyZXF1ZXN0Lmh0dHAucGFyYW1ldGVyLmxvb2t1cFVzZXJ9"/>
            <L7p:VariableToSet stringValue="lookupUser"/>
        </L7p:SetVariable>
        <L7p:ComparisonAssertion>
            <L7p:CaseSensitive booleanValue="false"/>
            <L7p:Expression1 stringValue="${xpathSynchToken.result}"/>
            <L7p:Operator operatorNull="null"/>
            <L7p:Predicates predicates="included">
                <L7p:item dataType="included">
                    <L7p:Type variableDataType="string"/>
                </L7p:item>
                <L7p:item binary="included">
                    <L7p:RightValue stringValue="${request.http.parameter.synchToken}"/>
                </L7p:item>
            </L7p:Predicates>
        </L7p:ComparisonAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHt4cGF0aFN5bmNoVG9rZW4ucmVzdWx0fQ=="/>
            <L7p:VariableToSet stringValue="synchToken"/>
        </L7p:SetVariable>
        <wsp:OneOrMore wsp:Usage="Required">
            <L7p:ComparisonAssertion>
                <L7p:CaseSensitive booleanValue="false"/>
                <L7p:Expression1 stringValue="${lookupUser}"/>
                <L7p:Expression2 stringValue=""/>
                <L7p:Negate booleanValue="true"/>
                <L7p:Operator operator="EMPTY"/>
                <L7p:Predicates predicates="included">
                    <L7p:item binary="included">
                        <L7p:CaseSensitive booleanValue="false"/>
                        <L7p:Negated booleanValue="true"/>
                        <L7p:Operator operator="EMPTY"/>
                        <L7p:RightValue stringValue=""/>
                    </L7p:item>
                </L7p:Predicates>
            </L7p:ComparisonAssertion>
            <L7p:SetVariable>
                <L7p:Base64Expression stringValue="JHtjdXJyZW50LnVzZXJuYW1lfQ=="/>
                <L7p:VariableToSet stringValue="lookupUser"/>
            </L7p:SetVariable>
        </wsp:OneOrMore>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue=""/>
            <L7p:VariableToSet stringValue="loggedinDiv"/>
        </L7p:SetVariable>
        <L7p:Regex>
            <L7p:AutoTarget booleanValue="false"/>
            <L7p:OtherTargetMessageVariable stringValue="website_top"/>
            <L7p:Regex stringValue="&lt;!--LOGGED_IN_USER_DIV-->"/>
            <L7p:Replace booleanValue="true"/>
            <L7p:Replacement stringValue="${loggedinDiv}"/>
            <L7p:Target target="OTHER"/>
        </L7p:Regex>
        <wsp:OneOrMore wsp:Usage="Required">
            <wsp:All wsp:Usage="Required">
                <L7p:ComparisonAssertion>
                    <L7p:CaseSensitive booleanValue="false"/>
                    <L7p:Expression1 stringValue="${current.user.role}"/>
                    <L7p:Expression2 stringValue="admin"/>
                    <L7p:Predicates predicates="included">
                        <L7p:item binary="included">
                            <L7p:CaseSensitive booleanValue="false"/>
                            <L7p:RightValue stringValue="admin"/>
                        </L7p:item>
                    </L7p:Predicates>
                </L7p:ComparisonAssertion>
            </wsp:All>
            <L7p:SetVariable>
                <L7p:Base64Expression stringValue="JHtjdXJyZW50LnVzZXJuYW1lfQ=="/>
                <L7p:VariableToSet stringValue="lookupUser"/>
            </L7p:SetVariable>
            <L7p:assertionComment>
                <L7p:Properties mapValue="included">
                    <L7p:entry>
                        <L7p:key stringValue="LEFT.COMMENT"/>
                        <L7p:value stringValue="admin"/>
                    </L7p:entry>
                    <L7p:entry>
                        <L7p:key stringValue="RIGHT.COMMENT"/>
                        <L7p:value stringValue="find out if the current user is an admin"/>
                    </L7p:entry>
                </L7p:Properties>
            </L7p:assertionComment>
        </wsp:OneOrMore>
        <L7p:ExportVariables>
            <L7p:ExportedVars stringArrayValue="included">
                <L7p:item stringValue="access_token"/>
                <L7p:item stringValue="current.user.role"/>
                <L7p:item stringValue="current.username"/>
                <L7p:item stringValue="loggedinDiv"/>
                <L7p:item stringValue="lookupUser"/>
                <L7p:item stringValue="userSession"/>
                <L7p:item stringValue="website_top"/>
            </L7p:ExportedVars>
        </L7p:ExportVariables>
    </wsp:All>
</wsp:Policy>