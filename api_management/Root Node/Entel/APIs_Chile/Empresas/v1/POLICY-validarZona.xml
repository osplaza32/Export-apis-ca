                <L7p:Regex>
                    <L7p:AutoTarget booleanValue="false"/>
                    <L7p:OtherTargetMessageVariable stringValue="bodyResult"/>
                    <L7p:Regex stringValue="&quot;message.+?,"/>
                    <L7p:RegexName stringValue="Elimina code"/>
                    <L7p:Replace booleanValue="true"/>
                    <L7p:Replacement stringValue=""/>
                    <L7p:Target target="OTHER"/>
                </L7p:Regex>
