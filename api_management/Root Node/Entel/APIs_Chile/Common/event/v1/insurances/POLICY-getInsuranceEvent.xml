<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:L7p="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:All wsp:Usage="Required">
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="serviceURI: URL completa del servicio de Bus"/>
        </L7p:CommentAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="aHR0cDovLyR7Z2F0ZXdheS5lVVNCLnRkZS5ob3N0bmFtZX0vRVMvR2V0SW5zdXJhbmNlRXZlbnQvdjE="/>
            <L7p:VariableToSet stringValue="serviceURI"/>
        </L7p:SetVariable>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="operationCode: Codigo de servicio de la pagina de confluence"/>
        </L7p:CommentAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="U0FTUzEwMDMzNQ=="/>
            <L7p:VariableToSet stringValue="operationCode"/>
        </L7p:SetVariable>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="body: Body que se envia a Bus, si es un POST o PUT se debe enviar ${request.mainpart}"/>
        </L7p:CommentAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHtyZXF1ZXN0Lm1haW5wYXJ0fQ=="/>
            <L7p:VariableToSet stringValue="body"/>
        </L7p:SetVariable>
        <L7p:CommentAssertion>
            <L7p:Comment stringValue="wsdl: WSDL del servicio, extraido desde SOAPUI"/>
        </L7p:CommentAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue=""/>
            <L7p:VariableToSet stringValue="wsdl"/>
        </L7p:SetVariable>
        <L7p:Include>
            <L7p:PolicyGuid stringValue="abead8d5-60ed-46b9-8eea-80ee1ea61820"/>
        </L7p:Include>
    </wsp:All>
</wsp:Policy>