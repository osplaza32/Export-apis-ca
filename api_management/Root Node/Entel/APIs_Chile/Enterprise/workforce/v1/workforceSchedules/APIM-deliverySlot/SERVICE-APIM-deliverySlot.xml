                                    </L7p:Predicates>
                                </L7p:ComparisonAssertion>
                                <L7p:Include>
                                    <L7p:PolicyGuid stringValue="9d5c486b-4dd0-4cf0-842c-0c048f753d0e"/>
                                </L7p:Include>
                            </wsp:All>
                            <wsp:All wsp:Usage="Required">
