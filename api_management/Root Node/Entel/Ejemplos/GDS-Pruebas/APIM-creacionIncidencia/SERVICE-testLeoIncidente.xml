<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:L7p="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:All wsp:Usage="Required">
        <L7p:HttpRoutingAssertion>
            <L7p:FailOnErrorStatus booleanValue="false"/>
            <L7p:ForceIncludeRequestBody booleanValue="true"/>
            <L7p:HttpMethod httpMethod="POST"/>
            <L7p:ProtectedServiceUrl stringValue="http://172.16.181.49:8080/arsys/services/ARService?server=${request.http.parameters.server}&amp;webService=${request.http.parameters.webService}"/>
            <L7p:ProxyPassword stringValueNull="null"/>
            <L7p:ProxyUsername stringValueNull="null"/>
            <L7p:RequestHeaderRules httpPassthroughRuleSet="included">
                <L7p:Rules httpPassthroughRules="included">
                    <L7p:item httpPassthroughRule="included">
                        <L7p:CustomizeValue stringValue="urn:HPD_Crea_Incidencia/HelpDesk_Submit_Service"/>
                        <L7p:Name stringValue="SOAPAction"/>
                        <L7p:UsesCustomizedValue booleanValue="true"/>
                    </L7p:item>
                </L7p:Rules>
            </L7p:RequestHeaderRules>
            <L7p:RequestParamRules httpPassthroughRuleSet="included">
                <L7p:ForwardAll booleanValue="true"/>
                <L7p:Rules httpPassthroughRules="included">
                    <L7p:item httpPassthroughRule="included">
                        <L7p:CustomizeValue stringValue="172.16.181.42"/>
                        <L7p:Name stringValue="server"/>
                        <L7p:UsesCustomizedValue booleanValue="true"/>
                    </L7p:item>
                    <L7p:item httpPassthroughRule="included">
                        <L7p:CustomizeValue stringValue="HPD_Crea_Incidencia"/>
                        <L7p:Name stringValue="webService"/>
                        <L7p:UsesCustomizedValue booleanValue="true"/>
                    </L7p:item>
                </L7p:Rules>
            </L7p:RequestParamRules>
            <L7p:ResponseHeaderRules httpPassthroughRuleSet="included">
                <L7p:ForwardAll booleanValue="true"/>
                <L7p:Rules httpPassthroughRules="included">
                    <L7p:item httpPassthroughRule="included">
                        <L7p:Name stringValue="Set-Cookie"/>
                    </L7p:item>
                </L7p:Rules>
            </L7p:ResponseHeaderRules>
            <L7p:ResponseMsgDest stringValue="resultRoute"/>
            <L7p:SamlAssertionVersion intValue="2"/>
        </L7p:HttpRoutingAssertion>
        <L7p:SetVariable>
            <L7p:Base64Expression stringValue="JHtyZXN1bHRSb3V0ZX0="/>
            <L7p:ContentType stringValue="text/xml; charset=utf-8"/>
            <L7p:DataType variableDataType="message"/>
            <L7p:VariableToSet stringValue="result"/>
        </L7p:SetVariable>
        <L7p:HardcodedResponse>
            <L7p:Base64ResponseBody stringValue="JHtyZXN1bHRSb3V0ZS5tYWlucGFydH0KCgo="/>
            <L7p:ResponseContentType stringValue="text/xml; charset=utf-8"/>
        </L7p:HardcodedResponse>
    </wsp:All>
</wsp:Policy>