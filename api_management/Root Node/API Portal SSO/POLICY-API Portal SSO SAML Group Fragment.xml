                    <L7p:Base64Expression stringValue="JHtzYW1sX2dyb3Vwc30="/>
                    <L7p:ContentType stringValue="application/json; charset=utf-8"/>
                    <L7p:DataType variableDataType="message"/>
                    <L7p:VariableToSet stringValue="samlMsg"/>
                </L7p:SetVariable>
                <L7p:JSONSchema>
                    <L7p:OtherTargetMessageVariable stringValue="samlMsg"/>
                    <L7p:ResourceInfo staticResourceInfo="included">
