                <L7p:ComparisonAssertion>
                    <L7p:Expression1 stringValue="${jsonPathOrg.results.length}"/>
                    <L7p:Expression2 stringValue="${jsonPathRole.results.length}"/>
                    <L7p:Predicates predicates="included">
                        <L7p:item binary="included">
                            <L7p:RightValue stringValue="${jsonPathRole.results.length}"/>
                        </L7p:item>
                    </L7p:Predicates>
                </L7p:ComparisonAssertion>
                <L7p:ForEachLoop L7p:Usage="Required"
                    loopVariable="jsonPathOrg.results" variablePrefix="i">
                    <L7p:EvaluateJsonPathExpression>
                        <L7p:Expression stringValue="$..organization[${i.iterations}]"/>
                        <L7p:OtherTargetMessageVariable stringValue="samlMsg"/>
                        <L7p:Target target="OTHER"/>
                        <L7p:VariablePrefix stringValue="jsonPathOrgSingle"/>
                    </L7p:EvaluateJsonPathExpression>
                    <L7p:EvaluateJsonPathExpression>
                        <L7p:Expression stringValue="$..role[${i.iterations}]"/>
                        <L7p:OtherTargetMessageVariable stringValue="samlMsg"/>
                        <L7p:Target target="OTHER"/>
                        <L7p:VariablePrefix stringValue="jsonPathRoleSingle"/>
                    </L7p:EvaluateJsonPathExpression>
                    <L7p:SetVariable>
                        <L7p:Base64Expression stringValue="JHt4bWwuZ3JvdXBzfQ0KCQk8TDdnaW1zOmF0dHI+DQoJCQk8TDdnaW1zOmF0dHItbmFtZT5tZW1iZXJPZjwvTDdnaW1zOmF0dHItbmFtZT4NCgkJCTxMN2dpbXM6YXR0ci12YWx1ZT48IVtDREFUQVske2pzb25QYXRoT3JnU2luZ2xlLnJlc3VsdH06Q049JHtqc29uUGF0aFJvbGVTaW5nbGUucmVzdWx0fV1dPjwvTDdnaW1zOmF0dHItdmFsdWU+DQoJCTwvTDdnaW1zOmF0dHI+DQo="/>
